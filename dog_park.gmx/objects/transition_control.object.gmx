            <kind>1</kind>
            <string>///Transition
if (keyboard_check_pressed(vk_space)) {
fade = true;
}

if(fade){
    timer += .5/room_speed;
    
    if (timer % 5)
    {
        if(counter &lt; width) spawn = true;
        counter++;       
    }
    
    if (spawn){
        for(var i = 0; i &lt; height; i++){
            xx = 32*counter;
            yy = 32*i;
            with(instance_create(0, 0, obj_transition)){
                xx = transition_control.xx;
                yy = transition_control.yy;
            }
        }
        spawn = false;        
    }
    
    if(counter-30 &gt; width){
        counter = -1;
        fade = false;
        timer = 1;
        room_goto(rando_room);
    }    
}

</string>
          </argument>
        </arguments>
