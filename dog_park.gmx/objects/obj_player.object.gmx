depth = -y;

// Change the direction you're facing &amp; Sprite Animation control

if(keyboard_check(vk_left)){
    image_xscale = -1;
    sprite_index = spr_corgi_w;
    image_speed = .4;
}
else if(keyboard_check(vk_right)){
    image_xscale = 1;
    sprite_index = spr_corgi_w;
    image_speed = .4;
}
else if(keyboard_check(vk_up)){
    sprite_index = spr_corgi_w;
    image_speed = .4;
}
else if(keyboard_check(vk_down)){
    sprite_index = spr_corgi_w;
    image_speed = .4;
}
else if(keyboard_check(vk_enter)){
    sprite_index = global.wuff;
    if(keyboard_check(vk_enter) &amp;&amp; image_xscale = -1){
    sprite_index = global.wuff2;
    }

}
else{
    sprite_index = spr_corgi;
    image_speed = .15;
}
</string>
